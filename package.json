{
  "name": "youtube-preview",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "pre-install": "npm install && bower install && npm run setup-env",
    "setup-env": "cp app/scripts.babel/env-example.js app/scripts.babel/env.js",
    "start": "gulp watch",
    "build": "gulp build --production",
    "package": "gulp build --production && gulp package"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.3",
    "gulp-chrome-manifest": "0.0.13",
    "gulp-clean-css": "^2.0.3",
    "gulp-eslint": "^2.0.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^2.4.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.1",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.0.8",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.2.0",
    "main-bower-files": "^2.11.1",
    "run-sequence": "^1.1.5",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
